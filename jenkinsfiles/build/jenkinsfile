#!groovy

pipeline {
    agent any

    tools {
        git "Default"
        maven "maven"
    }

    stages {
        stage('Build') {
            steps {
                // Get some code from a GitHub repository
                git 'https://github.com/kirikova-k/gs-rest-service.git'

                // Run Maven on a Unix agent.
                sh "mvn  -f gs-rest-service/initial/pom.xml clean install -P production"
            }

        }
    }
}
